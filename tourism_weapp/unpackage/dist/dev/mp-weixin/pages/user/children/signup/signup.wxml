<view><form><view class="cu-form-group margin-top"><view class="title">头像</view><block wx:if="{{!formData.avatar}}"><button class="avatar" style="margin:0;" open-type="chooseAvatar" data-event-opts="{{[['chooseavatar',[['onChooseAvatar',['$event']]]]]}}" bindchooseavatar="__e"><image src="{{defaultAvatarUrl}}"></image></button></block><block wx:else><view class="{{[formData.avatar?'':'cu-avatar radius bg-gray']}}"><image class="avatar" src="{{formData.avatar}}" mode></image></view></block></view><view class="cu-form-group margin-top"><view class="title">手机号</view><input placeholder="输入手机号" type="number" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['formData']]]]]}}" value="{{formData.phone}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">姓名</view><input placeholder="输入姓名" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['formData']]]]]}}" value="{{formData.name}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">性别</view><picker value="{{formData.sex}}" range="{{sexPicker}}" data-event-opts="{{[['change',[['PickerChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(formData.sex?formData.sex:'选择性别')+''}}</view></picker></view><view class="cu-form-group"><view class="title">年龄</view><input placeholder="输入年龄" type="number" data-event-opts="{{[['input',[['__set_model',['$0','age','$event',[]],['formData']]]]]}}" value="{{formData.age}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">身份证</view><input placeholder="输入身份证" type="number" data-event-opts="{{[['input',[['__set_model',['$0','idCard','$event',[]],['formData']]]]]}}" value="{{formData.idCard}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">密码</view><input placeholder="输入密码" type="password" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['formData']]]]]}}" value="{{formData.password}}" bindinput="__e"/></view></form><view class="form_item margin-top padding-left-sm"><text data-event-opts="{{[['tap',[['toSignin',['$event']]]]]}}" class="text-blue" bindtap="__e">已有账号？去登录</text></view><view class="action"><button data-event-opts="{{[['tap',[['onSubmit',['$event']]]]]}}" class="cu-btn lg bg-blue radius" bindtap="__e">注册</button></view></view>