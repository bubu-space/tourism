<view><swiper class="screen-swiper" indicator-dots="{{true}}" circular="{{true}}" autoplay="{{true}}" interval="3000" duration="500"><block wx:for="{{strategy.images}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><image src="{{item.url}}" mode="aspectFill"></image></swiper-item></block></swiper><view class="padding-sm text-df bg-white" style="padding-bottom:100px;"><view class="title text-xl">{{strategy.title}}</view><view class="margin-top-xs text-grey" style="white-space:pre-line;">{{strategy.describle}}</view><view class="margin-top-xl text-grey">{{strategy.createTime}}</view></view><view class="cu-bar tabbar bg-white foot"><view class="flex align-center flex-sub justify-center"><block wx:if="{{!isLike}}"><text data-event-opts="{{[['tap',[['handleStar',['$event']]]]]}}" class="cuIcon-like margin-left-sm" style="color:#c6c6c6;font-size:20px;" bindtap="__e"></text></block><block wx:else><text data-event-opts="{{[['tap',[['handleStar',['$event']]]]]}}" class="cuIcon-likefill margin-left-sm" style="color:red;font-size:20px;" bindtap="__e"></text></block><text class="margin-left-xs text-df">{{strategy.likeCount}}</text></view><view class="flex align-center flex-sub justify-center"><text class="cuIcon-attention margin-right-sm" style="color:#c6c6c6;font-size:20px;"></text><text>{{strategy.watchCount}}</text></view></view></view>